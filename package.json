{
  "name": "image-recog",
  "version": "1.0.0",
  "description": "Image Recognition and data extraction project",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "install-all": "concurrently \"npm install\" \"npm run client-install\"",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vHepp/image-recog.git"
  },
  "author": "Vincent Hepola, Sheraz Khan, Nathaniel Willison, Karl Schaft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vHepp/image-recog/issues"
  },
  "homepage": "https://github.com/vHepp/image-recog#readme",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "fs": "^0.0.1-security"
  }
}
